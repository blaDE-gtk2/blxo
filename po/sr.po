#: ../blxo-csource/main.c:303
#, c-format
msgid "Usage: %s [options] [file]\n"
msgstr "Употреба: %s [могућности] [датотека]\n"

#: ../blxo-csource/main.c:304
#, c-format
msgid "       %s [options] --build-list [[name file]...]\n"
msgstr "       %s [могућности] --build-list [[назив датотеке]...]\n"

#: ../blxo-csource/main.c:306
#, c-format
msgid "  -h, --help        Print this help message and exit\n"
msgstr "  -h, --help        Исписује ову поруку и излази\n"

#: ../blxo-csource/main.c:307
#, c-format
msgid "  -V, --version     Print version information and exit\n"
msgstr "  -V, --version     Исписује податке о издању и излази\n"

