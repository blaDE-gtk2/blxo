#, c-format
msgid "  --extern          Generate extern symbols\n"
msgstr "  --extern          Образује спољне симболе\n"

#: ../blxo-csource/main.c:309
#, c-format
msgid "  --static          Generate static symbols\n"
msgstr "  --static          Образује сталне симболе\n"

